<!-- This is a simple weather icon converted to ICO format -->
<!-- For production, you'd want to use a proper ICO file, but browsers support SVG favicons now -->
<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32 32" fill="none">
  <defs>
    <linearGradient id="sunGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#FFD700"/>
      <stop offset="100%" style="stop-color:#FFA500"/>
    </linearGradient>
    <linearGradient id="cloudGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#E6F3FF"/>
      <stop offset="100%" style="stop-color:#B8E0FF"/>
    </linearGradient>
  </defs>
  
  <!-- Sun -->
  <circle cx="12" cy="12" r="5" fill="url(#sunGrad)" stroke="#FFB347"/>
  
  <!-- Sun rays (simplified) -->
  <line x1="12" y1="3" x2="12" y2="1" stroke="#FFB347" stroke-width="1"/>
  <line x1="18" y1="6" x2="19" y2="5" stroke="#FFB347" stroke-width="1"/>
  <line x1="21" y1="12" x2="23" y2="12" stroke="#FFB347" stroke-width="1"/>
  <line x1="6" y1="6" x2="5" y2="5" stroke="#FFB347" stroke-width="1"/>
  <line x1="3" y1="12" x2="1" y2="12" stroke="#FFB347" stroke-width="1"/>
  
  <!-- Cloud -->
  <ellipse cx="22" cy="22" rx="8" ry="5" fill="url(#cloudGrad)" stroke="#87CEEB"/>
  <ellipse cx="17" cy="20" rx="5" ry="3.5" fill="url(#cloudGrad)" stroke="#87CEEB"/>
  <ellipse cx="26" cy="19" rx="4" ry="3" fill="url(#cloudGrad)" stroke="#87CEEB"/>
</svg>
